{
  "name": "alien-clown-ghost-robot-koala",
  "version": "1.0.0",
  "description": "homeliaison SI system <alien-clown-ghost-robot-koala>",
  "main": "robot.js",
  "scripts": {
    "robot": "node robot.js",
    "source": "node robot.js frontsource --entire",
    "app": "electron ./apps/nativeNotifier/app.js",
    "build": "electron-builder"
  },
  "author": "uragen",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@slack/web-api": "^5.15.0",
    "axios": "^0.21.1",
    "csso": "^4.2.0",
    "express": "^4.17.1",
    "express-useragent": "^1.0.15",
    "form-data": "^3.0.0",
    "formidable": "^2.0.1",
    "googleapis": "^51.0.0",
    "jsdom": "^16.4.0",
    "mongodb": "^3.6.3",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "shelljs": "^0.8.4",
    "terser": "^4.8.0",
    "ws": "^7.4.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-loader": "^8.2.2",
    "babel-preset-minify": "^0.5.1",
    "electron": "^16.0.2",
    "electron-builder": "^22.14.5"
  },
  "build": {
    "appId": "homeliaisonNotifier",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "category": "SI",
      "icon": "./apps/nativeNotifier/module/vendor/icon.icns"
    },
    "win": {
      "target": [
        "zip",
        "nsis"
      ],
      "icon": "./apps/nativeNotifier/module/vendor/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
